{
    "name": "xcom-o-pedia",
    "version": "1.0.0",
    "description": "React-based XCOM UFO Defense + TFTD UFOpaedia parser",
    "main": "main.js",
    "type": "module",
    "private": "true",
    "scripts": {
        "dev": "vite",
        "build": "node build.mjs",
        "min": "node build.mjs --minify",
        "watch": "node build.mjs --watch",
        "start": "node build.mjs --serve",
        "ws": "concurrently --kill-others \"node build.mjs --watch\" \"serve .. -p 2601\"",
        "neu": "neu run",
        "neub": "neu run -- --mode=browser",
        "neubuild": "npm run build && neu build --release"
    },
    "dependencies": {
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.1",
        "@fontsource/roboto": "^5.2.9",
        "@mui/icons-material": "^7.3.7",
        "@mui/material": "^7.3.7",
        "@tanstack/react-router": "^1.157.15",
        "concurrently": "^9.2.1",
        "country-flag-icons": "^1.6.8",
        "fuse.js": "^7.1.0",
        "js-worker-search": "^2.0.0",
        "js-yaml": "^4.1.1",
        "jszip": "^3.10.1",
        "lz-string": "^1.5.0",
        "open": "^11.0.0"
    },
    "devDependencies": {
        "@babel/plugin-transform-runtime": "^7.28.5",
        "@babel/preset-env": "^7.28.6",
        "@eslint/css": "^0.14.1",
        "@eslint/js": "^9.39.2",
        "@types/js-yaml": "^4.0.9",
        "@types/node": "^25.0.10",
        "@types/react": "^19.2.9",
        "@types/react-dom": "^19.2.3",
        "@vitejs/plugin-react": "^5.1.2",
        "eslint": "^9.39.2",
        "eslint-plugin-html": "^8.1.4",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^7.0.1",
        "eslint-plugin-react-refresh": "^0.4.26",
        "globals": "^17.1.0",
        "prettier": "^3.8.1",
        "sass": "^1.97.3",
        "stylelint": "^17.0.0",
        "stylelint-config-standard": "^40.0.0",
        "stylelint-config-standard-scss": "^17.0.0",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.54.0",
        "vite": "npm:rolldown-vite@7.3.1",
        "vite-plugin-stylelint": "^6.0.4"
    },
    "peerDependencies": {
        "react": "^17.0.0 || ^18.0.0 || ^19.0.0",
        "react-dom": "^17.0.0 || ^18.0.0 || ^19.0.0"
    },
    "overrides": {
        "vite": "npm:rolldown-vite@7.3.1"
    },
    "engines": {
        "node": ">=v25.2.0",
        "npm": ">=11.7.0"
    }
}
